  "bugs": {
    "url": "https://github.com/rainewallis/rollup-preact-typescript-tmpl/issues"
  },
  "homepage": "https://github.com/rainewallis/rollup-preact-typescript-tmpl#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/preact": "^2.0.1",
    "@types/jest": "^27.0.3",
    "autoprefixer": "^10.4.0",
    "find": "^0.3.0",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.0",
