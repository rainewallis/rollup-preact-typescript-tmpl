{
  "name": "rollup-preact-typescript-tmpl",
  "version": "1.0.0",
  "description": "A template repository for projects using Preact and Typescript packaged using Rollup.js",
  "main": "main.tsx",
  "scripts": {
    "test": "jest",
    "build": "rollup --config --environment NODE_ENV:development",
    "watch": "rollup --config --watch --environment NODE_ENV:development",
    "build:prod": "rollup --config --environment NODE_ENV:production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainewallis/rollup-preact-typescript-tmpl.git"
  },
  "keywords": [
